server{
    listen 80;
    server_name www.domain1.com;
    
    access_log /Users/dulin/workspace/nginx-winter/demo-3/logs/$server_name.access.log combined;

    location / {
        root /Users/dulin/workspace/nginx-winter/demo-3/html/www.domain1.com;
        index   index.html  index.htm;
    }

    #临时跳转
    location /redirect {
        return  302 http://www.google.com;
    }
    #临时跳转到about.html
    location /about_me/ {
        return 302 http://$host/about.html;
    }

    error_page  404 /404.html;

}